
@{
    ViewBag.Title = "InsertarUsuario";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
</head>
<body>
    <form method="post">
        <div class="frontbox">

            <div class="login">
                <h2>Insertar Usuario</h2>
                <div class="inputbox">
                    <input type="text" name="Nick" required placeholder="NOMBRE">
                    <input type="password" name="Pass" required placeholder="CONTRASEÑA">
                </div>
                <button name="boton" value="iniciar">Insertar</button>
            </div>
    </form>
</body>
</html>